<?xml version="1.0" encoding="UTF-8"?>
<component name="Suggestion" extends="Rectangle">
    <script type="text/brightscript" uri="Suggestion.brs" />
    <interface>
        <field id = "width" type = "float" onChange = "setWidth" alwaysNotify="true"/> 
        <field id = "height" type = "float" onChange = "setHeight" alwaysNotify="true"/>
        <field id = "itemContent" type = "node" onChange = "setItemContent" alwaysNotify="true"/>
        <field id = "focusPercent" type = "float" onChange = "setFocusPercent" alwaysNotify="true"/>
    </interface>
    <children>
        <Label id = "title" />
    </children>
</component>